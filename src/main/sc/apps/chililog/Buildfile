# This line tells SproutCore's CSS preprocessor what class names to target.
# Since your app has a theme named 'chililog', and it is based on SproutCore's
# Ace theme (named 'ace'), it is set to 'ace.chililog'.
config :chililog,

:css_theme => 'ace.chililog',

:required => [:sproutcore, 'sproutcore/statechart']

